<table>
<tbody>
<% @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.release_year %></td>
    <td><%= song.genre %></td>
    <td><%= song.artist_name %></td>
    <%= link_to "delete", method: :delete, date: {confirm: 'realiy?'} %>
     <%= link_to 'Delete', song, method: :delete, data: { confirm: 'For real?' } %>
  </tr>
<% end %>
</tbody>
</table>
