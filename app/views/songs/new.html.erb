<% if @songs.errors.any? %>
  <ul>
  <% @songs.errors.full_messages.each do |message|
    <li>message</li>
    <% end %>
  </ul>
  <% end %>
<%= form_for(@song) do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :released %>
  <%= f.text_field :release_year %>
  <%= f.text_field :artist_name %>
  <%= f.text_field :genre %>
  <%= f.submit %>
<% end %>
